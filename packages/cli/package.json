{
	"name": "@sodun/cli",
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"packageManager": "pnpm@7.5.1",
	"binName": "sodun",
	"bin": {
		"sodun": "./dist/index.js"
	},
	"scripts": {
		"start": "nodemon -e ts -w src --exec babel --extensions .ts src --out-dir dist",
		"build": "babel --extensions .ts src --out-dir dist",
		"test": "jest"
	},
	"keywords": [
		"sodun"
	],
	"author": "daihaoxin",
	"license": "ISC",
	"dependencies": {
		"@sodun/help-use-esm-in-nodejs": "workspace: *",
		"chalk": "4.1.2",
		"clear": "0.1.0",
		"commander": "9.3.0",
		"cross-spawn": "7.0.3",
		"figlet": "1.5.2",
		"handlebars": "4.7.7",
		"open": "8.4.0",
		"ora": "5.4.1"
	},
	"engines": {
		"node": ">= 16.0.0"
	},
	"eslintIgnore": [
		"./lib",
		"./dist"
	],
	"eslintConfig": {
		"parserOptions": {
			"babelOptions": {
				"rootMode": "upward"
			}
		}
	}
}
